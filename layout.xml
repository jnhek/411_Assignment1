<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:text="Rules void hello1(int hour)"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:background="#000000"
        />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="properties"
            android:textAlignment="center"
            android:textColor="#000000" />

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rowCount="2"
            android:columnCount="2"
            android:layout_weight="3"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:text="name"
                android:textAlignment="center"
                android:textColor="#000000" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:text="Day Hour Classification"
                android:layout_gravity="start"
                android:textAlignment="textStart"
                android:textColor="#000000" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:text="category"
                android:textAlignment="center"
                android:textColor="#000000" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_rowWeight="1"
                android:layout_columnWeight="1"
                android:text="Day and Time"
                android:layout_gravity="start"
                android:textAlignment="textStart"
                android:textColor="#000000" />
        </GridLayout>
    </LinearLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ADD8D7"
        android:columnCount="3"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_rowWeight="1"
            android:layout_columnWeight="1"
            android:text="Rule"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:layout_width="146dp"
            android:layout_height="wrap_content"
            android:layout_rowWeight="1"
            android:layout_columnWeight="1"
            android:text="C1"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_rowWeight="1"
            android:layout_columnWeight="1"
            android:text="A1"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:layout_width="51dp"
            android:layout_height="wrap_content"
            android:layout_rowWeight="1"
            android:layout_columnWeight="1"
            android:text=""
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="5pt" />


        <TextView
            android:layout_width="147dp"
            android:layout_height="wrap_content"
            android:layout_rowWeight="1"
            android:layout_columnWeight="1"
            android:text="min&#60;&#61;hour &#38;&#38; hour&#60;&#61;max"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="5pt"
            />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_rowWeight="1"
            android:layout_columnWeight="1"
            android:text="System.out.println(greeting + &#34;, World!&#34;)"
            android:textAlignment="center"

            android:textColor="#000000"
            android:textSize="5pt"
            />
    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:rowCount="1"
        android:columnCount="4"
        android:background="#ADD8D7"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:gravity="left"
            android:textColor="#000000"
            android:layout_columnWeight="2"
            android:layout_rowWeight="1"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="int min"
            android:gravity="center"
            android:textColor="#000000"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="int max"
            android:gravity="center"
            android:textColor="#000000"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="String greeting"
            android:gravity="center"
            android:textColor="#000000"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            />
    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:rowCount="1"
        android:columnCount="4"
        android:background="#FFFFFF"
        android:layout_columnWeight="1"
        android:layout_rowWeight="1"
        >

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rowCount="4"
            android:columnCount="1"
            android:background="#FFFFFF"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rule"
            android:gravity="left"
            android:textColor="#000000"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            android:textStyle="bold"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="R10"
            android:gravity="left"
            android:textColor="#000000"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="R20"
            android:gravity="left"
            android:textColor="#000000"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="R30"
            android:gravity="left"
            android:textColor="#000000"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="R40"
            android:gravity="left"
            android:textColor="#000000"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            />
        </GridLayout>

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rowCount="4"
            android:columnCount="1"
            android:background="#FFFFFF"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="From"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:textStyle="bold"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:background="#cfcb65"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="12"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:background="#cfcb65"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="18"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:background="#cfcb65"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="22"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:background="#cfcb65"
                />
        </GridLayout>

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rowCount="4"
            android:columnCount="1"
            android:background="#FFFFFF"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="To"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:textStyle="bold"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="11"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:background="#cfcb65"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="17"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:background="#cfcb65"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="21"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:background="#cfcb65"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="23"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:background="#cfcb65"
                />
        </GridLayout>

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rowCount="4"
            android:columnCount="1"
            android:background="#FFFFFF"
            android:layout_columnWeight="1"
            android:layout_rowWeight="1"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Greeting"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:textStyle="bold"
                android:background="#e8a766"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Good Morning"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:background="#e8a766"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Good Afternoon"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:background="#e8a766"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Good Evening"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:background="#e8a766"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Good Night"
                android:gravity="left"
                android:textColor="#000000"
                android:layout_columnWeight="1"
                android:layout_rowWeight="1"
                android:background="#e8a766"
                />
        </GridLayout>

    </GridLayout>
</LinearLayout>
